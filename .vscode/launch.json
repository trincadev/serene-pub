{
	"version": "0.2.0",
	"configurations": [
		{
			"type": "node",
			"request": "launch",
			"name": "Vite dev server",
			"program": "${workspaceFolder}/node_modules/vite/bin/vite.js",
			"args": ["--port", "5173", "--host"],
			"autoAttachChildProcesses": true,
			"env": {
				"NODE_ENV": "development"
			},
			"cwd": "${workspaceFolder}",
			"console": "integratedTerminal"
		}
	]
}
